<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:widget="urn:import:cn.mapway.ui.client.widget"
             xmlns:org="urn:import:cn.mapway.rbac.client.org">
    <g:DockLayoutPanel>
        <g:north size="50">
            <g:HTMLPanel styleName="ai-top">
                <widget:Header>人员列表</widget:Header>
                <g:HorizontalPanel ui:field="tools">
                    <g:Button ui:field="btnAdd">添加</g:Button>
                    <g:Button ui:field="btnDelete" enabled="false">删除</g:Button>
                </g:HorizontalPanel>
            </g:HTMLPanel>
        </g:north>
        <g:center>
            <g:ScrollPanel>
                <org:OrgUserList width="100%" ui:field="list"/>
            </g:ScrollPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>