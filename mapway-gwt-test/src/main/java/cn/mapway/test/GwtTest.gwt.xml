<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.0//EN"
        "http://gwtproject.org/doctype/2.9.0/gwt-module.dtd">
<module rename-to="test">

    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.user.theme.standard.Standard"/>
    <inherits name="cn.mapway.ui.MapwayBase" />
    <inherits name="cn.mapway.doc.openapi.OpenApi"/>

    <generate-with
            class="cn.mapway.ui.server.mvc.ModuleFactoryGenerator">
        <when-type-assignable
                class="cn.mapway.ui.client.mvc.ModuleFactory"/>
    </generate-with>

    <entry-point class='cn.mapway.test.client.GwtTest'/>

</module>