<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .box {
            display: flex;
            align-items: center;
            justify-content: center;
        }
    </ui:style>
    <g:DockLayoutPanel>
        <g:center>
            <g:HTMLPanel ui:field="list" styleName="{style.box}">
            </g:HTMLPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>