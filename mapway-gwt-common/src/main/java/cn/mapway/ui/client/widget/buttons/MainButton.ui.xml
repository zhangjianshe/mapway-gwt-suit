<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:widget="urn:import:cn.mapway.ui.client.widget">
    <ui:style>
        .img {
            vertical-align: bottom;
        }</ui:style>
    <g:HorizontalPanel styleName="ai-main-button" spacing="4" verticalAlignment="middle">
        <g:Image ui:field="icon" width="20px" height="20px" styleName="{style.img}"/>
        <widget:FontIcon ui:field="fontIcon" />
        <g:Label ui:field="txt"/>
    </g:HorizontalPanel>
</ui:UiBinder>
