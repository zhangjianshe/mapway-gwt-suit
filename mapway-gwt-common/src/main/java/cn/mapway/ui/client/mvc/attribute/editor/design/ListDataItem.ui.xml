<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:buttons="urn:import:cn.mapway.ui.client.widget.buttons"
             xmlns:widget="urn:import:cn.mapway.ui.client.widget">
    <ui:style>
        .box {
            display: flex;
            gap: 4px;
            margin-bottom: 4px;
        }
    </ui:style>
    <g:HTMLPanel styleName="{style.box}">
        <g:TextBox ui:field="txtKey" styleName="ai-input"/>
        <g:TextBox ui:field="txtValue" styleName="ai-input"/>
        <buttons:AiCheckBox ui:field="checkInit"/>
        <widget:FontIcon ui:field="btnDelete"/>
    </g:HTMLPanel>
</ui:UiBinder>